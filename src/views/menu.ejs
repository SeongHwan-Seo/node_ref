
<script type="text/javascript">
$(function(){
	if (navigator.userAgent.indexOf('iPhone')!=-1){
		addEventListener("load",function(){
			setTimeout(hideURLbar,0);
		},false);};
});

  


$(window).load(function(){
    
    menuExpanded = false;

    $('.menu-inner').on('click', function(){
		if( menuExpanded == false){
			$(this).parent().addClass('expanded');
			menuExpanded = true;
		}else{
			menuExpanded = false;
        	$(this).parent().removeClass('expanded');
		}
	});
});

function hideURLbar(){
	window.scrollTo(0,1);
};

</script>


<div id="menu" class="fl">
	<div class="menu-inner">
		<ul class="m_ul">

			<% if(userNm != ''){ %>
				<li><a class="noLine" href="/">Hello <%=userNm%>!</a></li>
			 <% } %>

			<li><a class="noLine" href="/apkView">APK 업로드</a></li>
			<li><a class="noLine" href="/apkDown">APK 다운로드</a></li>


			<% if(userNm != ''){ %>
				<ul class="m_ul">
				   <li><a class="noLine logOut" href="/logout">Log Out</a></li>   
				</ul>
			 <% } %>

		</ul>
	</div>
</div>
